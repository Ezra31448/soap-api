			"item": [
				{
					"name": "Register User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Test script for Register User",
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response contains success field\", function () {",
									"    var responseJson = xml2Json(pm.response.text());",
									"    pm.expect(responseJson['soap:Envelope']['soap:Body']['RegisterUserResponse']['success']).to.exist;",
									"});",
									"",
									"pm.test(\"User registered successfully\", function () {",
									"    var responseJson = xml2Json(pm.response.text());",
									"    var success = responseJson['soap:Envelope']['soap:Body']['RegisterUserResponse']['success'];",
									"    if (success === 'true' || success === true) {",
									"        // Store user ID and email for later tests",
									"        var userId = responseJson['soap:Envelope']['soap:Body']['RegisterUserResponse']['userId'];",
									"        var email = responseJson['soap:Envelope']['soap:Body']['RegisterUserResponse']['email'];",
									"        pm.collectionVariables.set('registeredUserId', userId);",
									"        pm.collectionVariables.set('registeredEmail', email);",
									"    }",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "text/xml; charset=utf-8"
							},
							{
								"key": "SOAPAction",
								"value": "http://example.com/usermanagement/RegisterUser"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<soap:Envelope xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\"\n               xmlns:tns=\"http://example.com/usermanagement\">\n    <soap:Header/>\n    <soap:Body>\n        <tns:RegisterUserRequest>\n            <tns:email>{{randomEmail}}</tns:email>\n            <tns:password>SecurePass123!</tns:password>\n            <tns:firstName>Test</tns:firstName>\n            <tns:lastName>User</tns:lastName>\n            <tns:phoneNumber>1234567890</tns:phoneNumber>\n        </tns:RegisterUserRequest>\n    </soap:Body>\n</soap:Envelope>"
